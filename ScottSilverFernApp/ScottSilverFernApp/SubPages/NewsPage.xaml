<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ScottSilverFernApp.NewsPage">
    <ContentView.Content>
        <ScrollView>
            <StackLayout BackgroundColor="#EBEBEB" Orientation="Vertical" VerticalOptions="Fill" HorizontalOptions="Fill" Spacing="0">

                <Image Source="new1.png" HorizontalOptions="Center" VerticalOptions="Center" Margin="10, 10, 10, 0">
                    <Image.GestureRecognizers>
                        <TapGestureRecognizer Tapped="TapGestureRecognizer_TappedNew1" />
                    </Image.GestureRecognizers>
                </Image>
                <Image Source="new2.png" HorizontalOptions="Center" VerticalOptions="Center" Margin="10, 0, 10, 0" >
